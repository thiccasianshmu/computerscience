public class FanTest {

	public static void main(String[] args) {

		Fan Fan1 = new Fan();
		Fan Fan2 = new Fan();

		Fan1.setFastSpeed();
		Fan1.setRadius(10);
		Fan1.setColor("Yellow");
		Fan1.setStatus(true);

		Fan2.setMediumSpeed();
		Fan2.setRadius(5); 
		Fan2.setColor("Blue"); 
		Fan2.setStatus(false); 

		System.out.println(Fan1.toString());
		System.out.println(Fan2.toString());

	}
}
